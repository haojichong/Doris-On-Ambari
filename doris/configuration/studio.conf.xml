<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration supports_final="true">
    <property>
        <name>STUDIO_PORT</name>
        <value>8899</value>
        <description>Studio http port</description>
    </property>
    <property>
        <name>MB_DB_TYPE</name>
        <value>h2</value>
        <description>部署依赖的数据服务类型支持mysql/h2/postgresql三种,默认支持h2</description>
    </property>
    <property>
        <name>MB_DB_HOST</name>
        <value>127.0.0.1</value>
        <description>如果要使用mysql或者postgresql,需要配置数据库IP.</description>
    </property>
    <property>
        <name>MB_DB_PORT</name>
        <value>3306</value>
        <description>数据库连接端口</description>
    </property>
    <property>
        <name>MB_DB_USER</name>
        <value>root</value>
        <description>连接访问用户</description>
    </property>
    <property>
        <name>MB_DB_PASS</name>
        <value>root</value>
        <description>密码</description>
    </property>
    <property>
        <name>MB_DB_DBNAME</name>
        <value>studio</value>
        <description>studio服务访问的数据库</description>
    </property>
    <property>
        <name>DEPLOY_NAME</name>
        <value>studio</value>
        <description>部署的服务名称，当前可以支持studio和manager</description>
    </property>
</configuration>




